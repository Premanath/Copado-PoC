                        "%vlocity_namespace%__Sass__c": "/*** CPQ Theme VARIABLES ***/\n@import \"cpq-theme-variables\";\n\n/*** MIXINS ***/\n@import \"cpq-mixin\";\n\n/*** STYLES ***/\n.vlocity {\n    .cpq-cart-item-child-products {\n        .slds-section {\n            margin: 0;\n\n            .slds-section__content {\n                padding-top: 0;\n            }\n        }\n\n        .cpq-cart-item-child-product {\n            display: flex;\n            flex-flow: row nowrap;\n            justify-content: flex-end;\n            padding: 0.1rem 0;\n\n            &.cpq-product-cart-item-child-selected {\n                &::after {\n                    content: '';\n                    display: block;\n                    position: absolute;\n                    top: 0;\n                    right: -$cpq-padding-large-horizontal;\n                    bottom: 0;\n                    // don't use left:0, as it will take full width rather than creating just the border to the right\n                    border-right: 8px solid $cpq-product-cart-item-selected-border-color;\n                }\n            }\n\n            .cpq-cart-item-child-product-name {\n                flex: 5;\n                padding: 0 $cpq-padding-base-horizontal/2 0 0;\n\n                .cpq-cart-item-child-product-name-wrapper {\n\n                    .cpq-cart-item-no-children{\n                        margin-top: 6px;\n                        padding-left: 21px;\n                    }\n                }\n\n                .cpq-cart-item-has-children {\n                    font-size: 13px;\n                    color: $mid-black;\n\n                    &:focus {\n                        box-shadow: none;\n                    }\n\n                    .slds-button__icon {\n                        float: left;\n                        margin: 0.3rem 0.5rem 0 0;\n                    }\n\n                    .cpq-product-name {\n                        line-height: 1.6;\n                        text-align: left;\n                        max-width: 85%;\n                        float: left;\n                        display: inline-block;\n                        margin-top: 0.125rem;\n                        margin-left: 0;\n                    }\n                }\n\n                .cpq-cart-item-no-children {\n                    margin-top: 6px;\n                }\n            }\n\n            .cpq-cart-item-child-product-input,\n            .cpq-cart-item-child-product-text {\n                flex: 2;\n                padding-right: $cpq-padding-base-horizontal/2;\n            }\n\n            .cpq-cart-item-child-product-text {\n                padding-top: $cpq-padding-base-vertical;\n                overflow-wrap: break-word;\n                min-width: 0;\n            }\n\n            .cpq-cart-item-child-product-price {\n                .slds-popover.cpq-price-actions-popover {\n                    position: absolute;\n                    z-index: 999;\n                    width: auto;\n                    top: $cpq-product-cart-item-popover-top;\n                    left: $cpq-product-cart-item-popover-left;\n\n                    .slds-popover__body.cpq-price-actions-popover__body {\n                        &:after {\n                            content: \"\";\n                            display: block;\n                            width: 100%;\n                            height: 2rem;\n                            position: absolute;\n                            left: 0;\n                            bottom: -2rem;\n                        }\n                    }\n                }\n            }\n\n            .cpq-cart-item-child-product-price {\n                flex: 2;\n                position: relative;\n                transition: color 250ms ease-in;\n                padding: $cpq-padding-base-vertical $cpq-padding-base-horizontal/2 0 0;\n\n            }\n\n            .cpq-cart-item-child-product-actions {\n                flex: 3;\n                padding-right: $cpq-padding-base-horizontal/2;\n\n                .cpq-button-spinner {\n                    position: relative;\n                    top: 15px\n                }\n\n                &.compacted {\n                    flex: 1;\n                    cursor: pointer;\n                }\n\n                .actions-dropdown-menu-dropdown {\n                    position: relative;\n\n                    .cpq-cart-item-actions-dropdown-button {\n                        height: 1.6875rem;\n                        margin-top: 0.125rem;\n\n                        .slds-button__icon {\n                            fill: $primary-medium;\n                        }\n                    }\n\n                    .cpq-action-item-label {\n                        font-size: 11px;\n                        color: $primary-medium;\n                    }\n\n                    .slds-icon {\n                        fill: $primary-medium;\n                        position: relative;\n                        top: -1px;\n                        margin-right: 0.25rem;\n                    }\n                }\n            }\n\n            .cpq-cart-item-child-product-name .slds-button__icon {\n                transform: rotate(0deg);\n                transform-origin: 45%;\n            }\n        }\n\n        .cpq-cart-item-product-group {\n            color: $cpq-cart-item-product-group-label-color;\n            font-weight: $cpq-cart-item-product-group-font-weight;\n            text-transform: uppercase;\n            padding: 4px 0;\n        }\n\n        .cpq-cart-item-currency-value {\n            color: $cpq-product-cart-items-attr-value-color;\n            padding-top: $cpq-padding-large-vertical/2;\n        }\n\n        .cpq-cart-item-input {\n            padding: 0 3px;\n            margin-top: $cpq-padding-large-vertical/5;\n            text-align: center;\n            width: 50px; //width can fit 3 digits\n            height: 25px;\n            min-height: calc(25px + 2px); //override slds input styles\n        }\n\n        .slds-section.slds-is-open .cpq-component-content {\n            display: block;\n        }\n\n        .fix-slds-close-switch {\n            transform: rotate(-90deg) !important;\n        }\n\n        .cpq-product-cart-item-section-heading {\n            font-size: $cpq-cart-section-title-font-size;\n        }\n\n        .cpq-cart-item-child-product-messages {\n            padding-left: 21px;\n            font-size: 95%;\n        }\n\n        .cpq-cart-item-child-product-cfg-attr {\n            .slds-section__title {\n                a {\n                    font-size: $cpq-cart-item-product-cfg-attr-font-size;\n                }\n\n                button {\n                    padding-bottom: 4px;\n                }\n            }\n\n            .cpq-cart-item-attributes-form {\n                .slds-section:first-child {\n                    .slds-section__title {\n                        margin-top: 8px;\n                    }\n                }\n            }\n        }\n\n        .underline{\n            border-bottom: dashed 1px;\n            &.hovered {\n                color: #0070d2;\n                cursor: pointer;\n            }\n        }\n    }\n\n    //Generate padding for product name label in tree\n    @include cpqProductChildLevelStyling();\n\n    // 64rem\n    @media screen and (max-width: $bp-large) {\n        .cpq-cart-item-child-products {\n            .cpq-cart-item-child-product {\n                .cpq-cart-item-child-product-actions {\n                    .slds-button {\n                        .slds-button__icon {\n                            fill: $primary-medium;\n                        }\n                    }\n                }\n            }\n        }\n    }\n}\n\n[dir=rtl] .vlocity {\n    .cpq-cart-item-child-products {\n        .cpq-cart-item-child-product-messages{\n            padding-left: 0;\n            padding-right: 2px; \n        }\n        .cpq-cart-item-child-product-cfg-attr {\n            .slds-section__title {\n                button.slds-button {\n                    .slds-button__icon {\n                        &.fix-slds-close-switch {\n                            transform: rotate(90deg) !important;\n                        }\n                    }\n                }\n            }\n        }\n\n        .cpq-cart-item-child-product {\n            .cpq-cart-item-child-product-name {\n                 padding: 0 0 0 6px;\n                .cpq-product-name{\n                    padding-left: 0;\n                }\n                .cpq-cart-item-has-children {\n                    .slds-button__icon {\n                        float: right; \n                        margin: 0.3rem 0 0 0.5rem;\n\n                        &.fix-slds-close-switch {\n                            transform: rotate(90deg) !important;\n                        }\n                    }\n                }\n               .cpq-cart-item-child-product-name-wrapper {\n\n                    .cpq-cart-item-no-children {\n                        padding-left: 0;\n                        padding-right: 21px;\n                    }\n                }\n\n            }\n            .cpq-cart-item-child-product-price {\n                .slds-popover.cpq-price-actions-popover {\n                    left: auto;\n                    right: -0.5rem;\n                }\n            }\n        }\n    }\n\n    @include cpqProductChildLevelStyling('right');\n}",
