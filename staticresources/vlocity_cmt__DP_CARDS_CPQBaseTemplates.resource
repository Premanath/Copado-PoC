                        "%vlocity_namespace%__HTML__c": "<div class=\"cpq-module\">\n\n    <!-- this div is to create a controller to be used in CustomJs to initialize CPQ API settings-->\n    <div ng-controller=\"initApiSettingsController\" />\n\n    <div class=\"slds-spinner_container\" ng-show=\"!isLoaded\">\n        <div class=\"slds-spinner--brand slds-spinner slds-spinner--medium\" aria-hidden=\"false\" role=\"alert\">\n            <div class=\"slds-spinner__dot-a\"></div>\n            <div class=\"slds-spinner__dot-b\"></div>\n        </div>\n    </div>\n\n    <div class=\"slds-grid slds-grid--frame slds-grid--vertical\">\n\n        <vloc-layout layout-name=\"cpq-header\"></vloc-layout>\n\n        <div class=\"slds-col slds-grid slds-wrap slds-small-nowrap cpq-container\" ng-class=\"{'cpq-outer-col-visible': !importedScope.showProductList || importedScope.isConfigAttrsPanelEnabled}\">\n            <div class=\"slds-backdrop cpq-tablet-overlay\" ng-if=\"importedScope.isMobileTablet\" ng-click=\"importedScope.toggleOutsideCols()\" ng-class=\"{'slds-backdrop--open': !importedScope.showProductList || importedScope.isConfigAttrsPanelEnabled}\"></div>\n\n            <div class=\"slds-is-relative slds-col slds-grid slds-size--1-of-1 slds-small-size--1-of-3 slds-medium-size--2-of-6 slds-large-size--4-of-12 cpq-left-sidebar-wrapper\" ng-class=\"{'cpq-left-sidebar-visible': !importedScope.showProductList && !importedScope.isConfigAttrsPanelEnabled}\">\n               \n                <div class=\"cpq-left-sidebar\">\n                    <button ng-if=\"importedScope.isMobileTablet\" class=\"slds-button slds-button--icon-border show-hide-button\" ng-click=\"importedScope.toggleOutsideCols()\">\n                        <slds-button-svg-icon sprite=\"'utility'\" size=\"'medium'\" icon=\"'right'\" extra-classes=\"'show-hide-button-icon'\"></slds-button-svg-icon>\n                    </button>\n                    <vloc-layout layout-name=\"cpq-left-sidebar\" ctrl=\"CPQItemsController\"></vloc-layout>\n                </div>\n\n            </div>\n\n            <div class=\"slds-is-relative slds-col slds-grid slds-size--1-of-1 cpq-cart-container-wrapper\" ng-class=\"{'slds-small-size--2-of-3 slds-medium-size--4-of-6 slds-large-size--8-of-12': importedScope.isConfigAttrsPanelEnabled || (!importedScope.showProductList && !importedScope.isConfigAttrsPanelEnabled), 'slds-small-size--3-of-3 slds-medium-size--6-of-6 slds-large-size--12-of-12': importedScope.showProductList && !importedScope.isConfigAttrsPanelEnabled}\">\n\n                <!-- Hide/Show Product List -->\n                <button ng-if=\"!importedScope.isMobileTablet\" class=\"slds-button slds-button--icon-border show-hide-button\" ng-class=\"{'cursor-point-west': !importedScope.showProductList, 'cursor-point-east': importedScope.showProductList}\" ng-click=\"importedScope.toggleOutsideCols()\">\n                </button>\n\n                <div class=\"cpq-cart-container\">\n                    <vloc-layout layout-name=\"cpq-product-cart\" ctrl=\"CPQCartController\" show-product-list=\"{{importedScope.showProductList}}\" show-config-panel=\"{{importedScope.isConfigAttrsPanelEnabled}}\"></vloc-layout>\n                </div>\n\n                <div>\n                    <vloc-layout layout-name=\"cpq-total-bar\" ctrl=\"CPQTotalController\" class=\"cpq-product-cart-order\"></vloc-layout>\n                </div>\n\n            </div>\n            \n            <div class=\"slds-is-relative slds-col slds-grid slds-size--1-of-1 slds-small-size--1-of-3 slds-medium-size--2-of-6 slds-large-size--4-of-12 cpq-product-container-wrapper\" ng-class=\"{'cpq-product-container-visible': importedScope.isConfigAttrsPanelEnabled}\">\n\n                <div class=\"cpq-product-container\">\n                    <vloc-layout layout-name=\"cpq-product-cart-config\" ctrl=\"CPQCartItemConfigController\"></vloc-layout>\n                </div>\n\n            </div>\n\n        </div>\n\n    </div>\n\n</div>",
                        "%vlocity_namespace%__Sass__c": "/*** CPQ THEME VARIABLES ***/\n@import \"cpq-theme-variables\";\n\n/*** MIXINS ***/\n@import \"cpq-mixin\";\n\n/*** STYLES ***/\n.vlocity {\n    .cpq-module {\n        background-color: $cpq-page-bg-color;\n    }\n\n    .cpq-container {\n        padding: $cpq-padding-large-horizontal $cpq-padding-large-horizontal/2;\n        height: calc(100vh - 81px);\n    }\n\n    .cpq-left-sidebar-wrapper {\n        transition: margin-left 250ms ease-in;\n        margin-left: -33.33%;\n\n        &.cpq-left-sidebar-visible {\n            margin-left: 0;\n        }\n    }\n\n    .cpq-cart-container-wrapper {\n        transition: width 250ms ease-in;\n\n    }\n\n    .cpq-product-container-wrapper {\n        transition: margin-right 250ms ease-in;\n        margin-right: -33.33%;\n\n        &.cpq-product-container-visible {\n            margin-right: 0;\n        }\n    }\n\n    .show-hide-button {\n        margin-left: -10px;\n        width: 10px;\n        height: auto; /* IE */\n        height: initial;\n        border-radius:0px;\n        border: none;\n        color: $black;\n        font-weight: 600;\n        font-size: 40px;\n\n        &:focus {\n            box-shadow: none;\n        }\n\n        &.cursor-point-west {\n            cursor: w-resize;\n        }\n\n        &.cursor-point-east {\n            cursor: e-resize;\n        }\n    }\n\n    // 64rem\n    @media screen and (max-width: $bp-large) {\n        .cpq-module {\n            input {\n                -webkit-appearance: none;\n            }\n        }\n\n        .cpq-container {\n            padding: 1rem 0 0;\n            position: absolute;\n            width: 100%;\n            bottom: 0;\n            left: 0;\n            overflow: hidden;\n\n            .cpq-tablet-overlay.slds-backdrop {\n                z-index: 990;\n                transition: opacity 250ms ease-in,\n                            visibility 0ms linear 250ms;\n            }\n            &.cpq-outer-col-visible {\n                .cpq-tablet-overlay.slds-backdrop--open {\n                    transition: opacity 250ms ease-in;\n                }\n            }\n        }\n        \n        .cpq-left-sidebar-wrapper {\n            transition-property: left;\n            position: fixed;\n            z-index: 999;\n            margin-left: 0;\n            left: -33.33%;\n            top: 0;\n            height: 100%;\n            background-color: $cards-gray;\n\n            ng-include {\n                display: block;\n            }\n\n            .cpq-left-sidebar {\n                width: 100%;\n                position: relative;\n\n                button.show-hide-button {\n                    position: absolute;\n                    right: -10px;\n                    z-index: 9999;\n                    top: 50%;\n                    transform: translateY(-50%);\n                    width: 2.75rem;\n                    height: 2.75rem;\n                    right: -2.75rem;\n                    background-color: rgba(0,0,0,0.5);\n                    border-top-right-radius: .25rem;\n                    border-bottom-right-radius: .25rem;\n\n                    > .slds-button__icon.show-hide-button-icon {\n                        position: absolute;\n                        top: 50%;\n                        left: 50%;\n                        transform: translateY(-50%) translateX(-50%);\n                        fill: #fff;\n                        width: 1.25rem;\n                        height: 1.25rem;\n                        transition: transform 500ms ease-in;\n                    } \n                }\n            }\n\n            &.cpq-left-sidebar-visible {\n                margin-left: 0;\n                left: 0;\n                box-shadow: 1px 0 5px rgba(0,0,0,0.3);\n\n                .cpq-left-sidebar {\n                    button.show-hide-button {\n                        > .slds-button__icon.show-hide-button-icon {\n                            transform: translateY(-50%) translateX(-50%) rotate(180deg);\n                        }\n                    }\n                }\n            }\n        }\n\n        .cpq-cart-container-wrapper {\n\n        }\n\n        .cpq-product-container-wrapper {\n            transition-property: right;\n            position: fixed;\n            z-index: 999;\n            margin-right: 0;\n            right: -33.33%;\n            top: 0;\n            height: 100%;\n            overflow-y: auto;\n            background-color: #fff;\n\n            ng-include {\n                display: block;\n            }\n\n            &.cpq-product-container-visible {\n                margin-right: 0;\n                right: 0;\n                box-shadow: 0 1px 5px rgba(0,0,0,0.3);\n            }\n        }\n    }\n\n    // 48rem\n    @media screen and (max-width: $bp-medium) {\n\n    }\n\n    // 30rem\n    @media screen and (max-width: $bp-small) {\n\n    }\n}\n\n[dir=rtl] .vlocity {\n    .cpq-left-sidebar-wrapper {\n        transition-property: margin-right;\n        margin-right: -33.33%;\n        margin-left: 0;\n\n        &.cpq-left-sidebar-visible {\n            margin-right: 0;\n        }\n    }\n    .cpq-product-container-wrapper {\n        transition-property: margin-left;\n        margin-left: -33.33%;\n        margin-right: 0;\n\n        &.cpq-product-container-visible {\n            margin-left: 0;\n        }\n    }\n    .show-hide-button {\n        margin-left: 0;\n        position: relative;\n        left: 8px;\n        margin-right:0;\n    }\n\n    // 64rem\n    @media screen and (max-width: $bp-large) {\n        .cpq-left-sidebar-wrapper {\n            transition-property: right;\n            right: -33.33%;\n            left: auto;\n            margin-right: 0;\n\n            .cpq-left-sidebar {\n                button.show-hide-button {\n                    left: -2.75rem;\n                    right: auto;\n                    border-radius: 0.25rem 0 0 0.25rem;\n\n                    > .slds-button__icon.show-hide-button-icon {\n                        transform: translateY(-50%) translateX(-50%) rotate(180deg);\n                    }\n                }\n            }\n\n            &.cpq-left-sidebar-visible {\n                right: 0;\n                left: auto;\n\n                .cpq-left-sidebar {\n                    button.show-hide-button {\n                        > .slds-button__icon.show-hide-button-icon {\n                            transform: translateY(-50%) translateX(-50%);\n                        }\n                    }\n                }\n            }\n        }\n        .cpq-product-container-wrapper {\n            transition-property: left;\n            left: -33.33%;\n            right: auto;\n            margin-left: 0;\n\n            &.cpq-product-container-visible {\n                left: 0;\n                right: auto;\n            }\n        }\n    }\n}\n",
