                        "%vlocity_namespace%__HTML__c": "<div class=\"slds-grid slds-grid--vertical cpq-product-cart\">\n    <div class=\"slds-spinner_container\" ng-show=\"!isLoaded\">\n        <div class=\"slds-spinner--brand slds-spinner slds-spinner--medium\" aria-hidden=\"false\" role=\"alert\">\n            <div class=\"slds-spinner__dot-a\"></div>\n            <div class=\"slds-spinner__dot-b\"></div>\n        </div>\n    </div>\n    <div class=\"slds-col slds-no-flex cpq-product-cart-global-tabs\" ng-init=\"tabSelected = 'Cart'\">\n        <ul class=\"slds-tabs--default__nav cpq-product-cart-tabs\" role=\"tablist\">\n            <li class=\"slds-tabs--default__item slds-text-heading--label\" title=\"Item One\" role=\"presentation\" ng-class=\"{'slds-active' : tabSelected === 'Cart'}\" ng-click=\"tabSelected = 'Cart'; importedScope.changePricingView(0)\">\n                <a class=\"slds-tabs--default__link\" href=\"#voi\" role=\"tab\" tabindex=\"0\" aria-selected=\"tabSelected === 'Cart'\" aria-controls=\"tab-default-1\" id=\"tab-default-1__item\">{{::importedScope.customLabels.CPQCart}}</a>\n            </li>\n            <li class=\"slds-tabs--default__item slds-text-heading--label\" title=\"Item One\" role=\"presentation\" ng-class=\"{'slds-active' : tabSelected === 'Pricing'}\" ng-click=\"tabSelected = 'Pricing';  importedScope.changePricingView(importedScope.currentPricingView)\" ng-show=\"$root.pricingViews.cpqPricingViews.length > 1\" ng-if=\"$root.vlocityCPQ.features.enablePricing\">\n                <a class=\"slds-tabs--default__link\" href=\"#voi\" role=\"tab\" tabindex=\"0\" aria-selected=\"tabSelected === 'Pricing'\" aria-controls=\"tab-default-1\" id=\"tab-default-1__item\">{{::importedScope.customLabels.CPQCartPricing}}</a>\n            </li>\n          <!--   <li class=\"slds-tabs--default__item slds-text-heading--label\" title=\"Item One\" role=\"presentation\" ng-class=\"{'slds-active' : tabSelected === 'Promotions'}\" ng-click=\"tabSelected = 'Promotions'\">\n                <a class=\"slds-tabs--default__link\" href=\"#voi\" role=\"tab\" tabindex=\"0\" aria-selected=\"tabSelected === 'Promotions'\" aria-controls=\"tab-default-1\" id=\"tab-default-1__item\">{{::importedScope.customLabels.CPQCartPricing}}</a>\n            </li> -->\n        </ul>\n    </div>  \n    <!--The scroll Class is used for mobile devices -->\n    <!-- js-cpq-cart-scroll-container unique class is used to scroll the container to an item -->\n    <div class=\"slds-col cpq-product-cart-items js-cpq-cart-scroll-container scroll\" ng-show=\"tabSelected === 'Cart' || tabSelected === 'Pricing'\">\n        <!--SHOW CART ERRORS-->\n        <div class=\"errors-container-sticky\" ng-show=\"importedScope.cartDataStore.filteredMessages.length > 0\">\n            <div class=\"cart-errors slds-section__title border-radius\"\n            ng-class=\"{'slds-theme--error': importedScope.hasSeverity == 'ERROR', 'slds-theme--warning': importedScope.hasSeverity == 'WARN', 'slds-theme--alert': importedScope.hasSeverity == 'INFO' }\"\n            ng-show=\"importedScope.cartDataStore.filteredMessages.length > 1\">\n                <a href=\"javascript:void(0);\" ng-click=\"showMessages = !showMessages\" class=\"slds-section__title-action cpq-product-cart-item-section-heading\">\n                    <button type=\"button\" class=\"slds-button slds-button--icon-container slds-button--icon-x-small\">\n                        <slds-button-svg-icon sprite=\"'utility'\" icon=\"'switch'\" ng-class=\"{'fix-slds-close-switch' : !showMessages}\"></slds-button-svg-icon>\n                    </button>\n                    {{::importedScope.customLabels.CPQCartMessages}} ({{importedScope.cartDataStore.filteredMessages.length}})\n                </a>\n            </div>\n            <div class=\"slds-notify cart-errors\" role=\"alert\" ng-repeat=\"msg in importedScope.cartDataStore.filteredMessages\"\n                ng-show=\"(importedScope.cartDataStore.filteredMessages[0].code != '101' && importedScope.cartDataStore.filteredMessages.length == 1 && records.length > 0) || showMessages \"\n                ng-init=\"msg.messageId = msg.messageId || msg.actions.DETAILS.client.params.id; importedScope.getCartSeverity();\"\n                ng-class=\"{'slds-theme--error': msg.severity == 'ERROR', 'slds-theme--warning': msg.severity == 'WARN', 'slds-theme--alert': msg.severity == 'INFO', 'border-radius': importedScope.cartDataStore.filteredMessages.length == 1 }\">\n                <div class=\"slds-notify__content slds-grid\">\n                    <div class=\"slds-col slds-align-middle\" >\n                        <h2 class=\"slds-text-heading--small\">{{msg.message}}</h2>\n                    </div>\n                    <button class=\"slds-button slds-p-horizontal--xx-small\" ng-click=\"importedScope.openDetailView(msg)\" ng-if=\"msg.actions['DETAILS']\">\n                        <slds-button-svg-icon sprite=\"'utility'\" icon=\"'preview'\"></slds-button-svg-icon>\n                        <span class=\"slds-assistive-text\">{{::importedScope.customLabels.CPQDetails}}</span>\n                    </button>\n                </div>\n            </div>\n        </div>\n\n        <!--END CART ERRORS-->\n        <div ng-class=\"{'cart-container-padding':importedScope.cartDataStore.filteredMessages.length > 0 && records.length > 0}\">\n            <div class=\"slds-picklist slds-dropdown-trigger slds-dropdown-trigger--click slds-is-open slds-m-around--small pricing-view-dropdown\" ng-show=\"tabSelected === 'Pricing' && $root.pricingViews.cpqPricingViews\" ng-if=\"$root.vlocityCPQ.features.enablePricing\">\n                <div class=\"slds-form-element\">\n                    <label class=\"slds-form-element__label\" for=\"cpq-pricing-view-button\">{{::importedScope.customLabels.CPQCartPricingViews}}</label>\n                </div>\n                <button cpq-dropdown-handler=\"importedScope.viewOpen = false\" id=\"cpq-pricing-view-button\" class=\"slds-button pricing-view-dropdown-button slds-button--neutral slds-picklist__label slds-p-right--small\" aria-haspopup=\"true\" ng-click=\"importedScope.viewOpen = !importedScope.viewOpen\">\n                    <slds-button-svg-icon sprite=\"'utility'\" icon=\"'down'\" extra-classes=\"'slds-m-top--x-small slds-float--right pricing-view-dropdown-button'\"></slds-button-svg-icon>\n                    <span class=\"slds-truncate\" title=\"Select an Option\"><span ng-if=\"!importedScope.pricingViewLabel\">{{$root.pricingViews.cpqPricingViews[1].label}}</span>\n                    <span ng-if=\"importedScope.pricingViewLabel\">{{importedScope.pricingViewLabel}}</span>\n                </button>\n                <div class=\"slds-dropdown slds-dropdown--left\" ng-if=\"importedScope.viewOpen\">\n                    <ul class=\"slds-dropdown__list slds-dropdown--length-5\" role=\"menu\">\n                        <li class=\"slds-dropdown__item\" role=\"presentation\" ng-repeat=\"pricingView in $root.pricingViews.cpqPricingViews\" ng-click=\"importedScope.changePricingView($index)\">\n                            <a ng-if=\"!$first\" href=\"javascript:void(0);\" role=\"menuitemcheckbox\" tabindex=\"0\">\n                                <span class=\"slds-truncate\">{{pricingView.label}}</span>\n                            </a>\n                        </li>\n                    </ul>\n                </div>\n            </div>\n            <div class=\"slds-tabs--default\" ng-class=\"{'slds-m-top--small': tabSelected === 'Cart'}\">\n                <div id=\"tab-default-1\" class=\"slds-tabs--default__content cpq-product-cart-items-tab-content slds-show\" role=\"tabpanel\" aria-labelledby=\"tab-default-1__item\">\n                    <vloc-card ng-repeat=\"card in cards\" ctrl=\"CPQCartItemController\" ng-model=\"card\" data=\"card\" records=\"records\" index=\"{{$index}}\" show-product-list=\"{{attrs.showProductList}}\" show-config-panel=\"{{attrs.showConfigPanel}}\"  ng-if=\"isLoaded\" is-last=\"{{$last}}\"></vloc-card>\n                    <div class=\"slds-grid slds-grid--vertical-align-center slds-grid--align-center cpq-no-cart-items-msg\" ng-if=\"!records || records.length === 0 \">\n                        {{::importedScope.customLabels.CPQCartIsEmpty}}\n                    </div>\n                     <div class=\"next-product slds-col slds-text-align--center slds-m-vertical--medium\" ng-if=\"session.nextProducts\">\n                        <div class=\"slds-spinner_container\" ng-show=\"loadingMore\">\n                            <div class=\"slds-spinner--brand slds-spinner slds-spinner--small\" aria-hidden=\"false\" role=\"alert\">\n                                <div class=\"slds-spinner__dot-a\"></div>\n                                <div class=\"slds-spinner__dot-b\"></div>\n                            </div>\n                        </div>\n                        <a ng-click=\"importedScope.nextPage()\" ng-if=\"!loadingMore\">\n                          {{::importedScope.customLabels.CPQLoadMore}}\n                        </a>\n                    </div>\n                </div>\n            </div>\n        </div>\n            <div id=\"tab-default-2\" class=\"slds-tabs--default__content cpq-product-cart-items-tab-content slds-hide\" role=\"tabpanel\" aria-labelledby=\"tab-default-2__item\">{{::importedScope.customLabels.CPQCartTabTwoContent}}</div>\n            <div id=\"tab-default-3\" class=\"slds-tabs--default__content cpq-product-cart-items-tab-content slds-hide\" role=\"tabpanel\" aria-labelledby=\"tab-default-3__item\">{{::importedScope.customLabels.CPQCartTabThreeContent}}</div>\n        </div>\n    </div>\n    <!--<vloc-layout layout-name=\"cpq-total-bar\" ctrl=\"CPQTotalController\" class=\"cpq-product-cart-order\"></vloc-layout>-->\n</div>",
                        "%vlocity_namespace%__Sass__c": "/*** CPQ Theme VARIABLES ***/\n@import \"cpq-theme-variables\";\n\n/*** MIXINS ***/\n@import \"cpq-mixin\";\n\n/*** STYLES ***/\n.vlocity {\n    .cpq-product-cart {\n        position: absolute;\n        top: 0;\n        right: $cpq-padding-large-horizontal/2;\n        bottom: 0;\n        left: $cpq-padding-large-horizontal/2;\n        height: calc(100% - 80px);\n\n        .errors-container-sticky {\n            position: absolute;\n            z-index: 1;\n            width: 100%;\n            top: 31px; /* fix for Firefox */\n            .border-radius{\n                border-radius: 4px 4px 0 0;\n            }\n        }\n        .cart-container-padding{\n            padding-top: 40px;\n        }\n        .cart-errors {\n            padding: $cpq-padding-large-vertical $cpq-padding-large-horizontal;\n            color: white;\n            h2 {\n                font-size: 14px;\n            }\n            a {\n                text-decoration: none;\n                font-weight: 300;\n                font-size: 1rem;\n                line-height: 1.25;\n                background-color: inherit;\n            }\n            .slds-button{\n                line-height: 1.25;\n                color: white;\n            }\n\n        }\n    }\n    .cpq-product-cart-tabs {\n        margin-top: $cpq-cart-container-tabs-top-margin;\n    }\n    .cpq-product-cart-items {\n        background-color: $cpq-product-cart-items-bg-color;\n        overflow-y: auto;\n        .cpq-product-cart-items-tab-content {\n            padding-top: 0px; //slds override\n            .cpq-no-cart-items-msg {\n                padding: $cpq-padding-large-vertical*4 $cpq-padding-large-vertical;\n            }\n        }\n        .next-product {\n            padding: $cpq-padding-large-vertical;\n\n            .slds-spinner_container {\n                position: relative;\n                padding-top: 10px;\n            }\n        }\n    }\n\n    .fix-slds-close-switch {\n        transform: rotate(-90deg);\n    }\n\n    // 64rem\n    @media screen and (max-width: $bp-large) {\n        .cpq-product-cart {\n            left: 0;\n            right: 0;\n\n            .cpq-product-cart-global-tabs {\n                ul.cpq-product-cart-tabs {\n                    padding-left: 1.5rem;\n                }\n            }\n        }\n    }\n}\n\n[dir=rtl] .vlocity {\n    // 64rem\n    @media screen and (max-width: $bp-large) {\n        .cpq-product-cart {\n            .cpq-product-cart-global-tabs {\n                ul.cpq-product-cart-tabs {\n                    padding-left: 0;\n                    padding-right: 1.5rem;\n                }\n            }\n        }\n    }\n}",
